		  "#define MAX 1e7+100",
		  "vector<int> isPrime(MAX, 1);",
		  "void SieveOfEratosthenes(){ ",
		  "    isPrime[0]=0;",
		  "    isPrime[1]=0;",
		  "    int n = MAX;",
		  "    for(int p=2; p*p<=n; p++){ ",
		  "        if(isPrime[p] == 1){  ",
